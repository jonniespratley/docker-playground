
server {
    # Note that it's listening on port 9000
    listen 9000 default_server;
    root /var/www/;
    index index.html index.htm;

    server_name example.com www.example.com;

    charset utf-8;
    include h5bp/basic.conf;

    location /public {
      alias /src/app/public;
    }
    location / {
        try_files $uri $uri/ =404;
    }

  
}